{
  "name": "klee-atlas",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "mock:api":" json-server ./MOCK_API/data.json -p 5050 -w ",
    "atlas:start": "ng serve --project=klee-atlas -o",
    "atlas:build": "ng build --project=klee-atlas",
    "atlas:test": "ng test --project=klee-atlas",
    "atlas:doc": "compodoc -p ./projects/klee-atlas/tsconfig.doc.json -s -o --output ./documentation/klee-atlas --disableCoverage",
    "preatlas:stats": "ng build --project=klee-atlas --stats-json",
    "atlas:stats": "webpack-bundle-analyzer ./dist/klee-atlas/stats.json",
    "klee:build": "ng build --project=klee",
    "preklee:pack": "npm run klee:build",
    "klee:pack": "cd ./distributions && npm pack ../dist/klee && cd ..",
    "klee:test": "ng test --project=klee",
    "gen": "npx ng-swagger-gen -i ./swagger/api.json -o ./swagger/generated"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.1.0",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/router": "^15.1.0",
    "primeicons": "^6.0.1",
    "primeng": "^15.2.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.5",
    "@angular/cli": "~15.1.5",
    "@angular/compiler-cli": "^15.1.0",
    "@compodoc/compodoc": "^1.1.19",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "json-server": "^0.17.3",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.1.0",
    "ng-swagger-gen": "^2.3.1",
    "typescript": "~4.9.4",
    "webpack-bundle-analyzer": "^4.8.0"
  }
}
